@{
    ViewBag.Title = Sic.Apollo.Resources.Resources.LabelForAppointmentRated;
}
<h1>@Sic.Apollo.Resources.Resources.LabelForAppointmentRated</h1>
<table>
    <tr>
        <td>            
            <p>@Sic.Apollo.Resources.Resources.LegendForAppointmentRated</p>            
        </td>        
    </tr>
    <tr><td>@Sic.Apollo.Resources.Resources.LabelForAppointmentRatedTweetText @TwitterGoodies.TweetButton(dataCount: TwitterGoodies.DataCount.None,
tweetText: String.Format(Sic.Apollo.Resources.Resources.LegendForAppointmentRatedTweetText, Sic.Apollo.Resources.Resources.LabelForApplicationUrl, ViewBag.FullName, ViewBag.Rate, Sic.Apollo.Resources.Resources.LabelForApplicationTwitterAccount),
        language: TwitterGoodies.Languages.Spanish)</td></tr>
</table>

<div>
    @Html.ActionLink(Sic.Apollo.Resources.Resources.LabelForPendingRatings, "CustomerPendingRatings")
</div>
