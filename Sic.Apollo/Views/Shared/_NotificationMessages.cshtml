@{
    Sic.Data.MessageCollection messages = (Sic.Data.MessageCollection)ViewBag.MessageCollection;        
}
@if(messages!=null && messages.Any())
{ 
<script>
    $(function(){
    @foreach (Sic.Data.Message message in messages)
    {
        <text>
        sicNotifyAsPopup('@message.Text', @((int)message.MessageType), '@message.Title');
        </text>
    }
    });
</script>
}