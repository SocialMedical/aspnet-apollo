@model Sic.Apollo.Models.General.Contact
@{
    string emptyPicture = "/Content/images/contacts/DefaultProfessional.jpg";
    string picture = emptyPicture;
    if (!string.IsNullOrEmpty(Model.Picture))
    {
        picture = Model.Picture;
    }
}
@section Imports
{
    <link href="@Url.ContentVersion("~/Scripts/fancy/jquery.fancybox.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.ContentVersion("~/Scripts/fancy/jquery.fancybox.pack.js")" type="text/javascript"></script>
}

<div id="profilePicture">
<a class="fancyboxpicture" href="@picture" title="@Model.FullName">
<img id="imageprofile" src="@picture" alt="@Model.FullName"  style = "width: 120px; height: 150px;" }/>
</a>
</div>


<script>
    $(".fancyboxpicture").fancybox();
</script>
